{
  "author": "Guy Ellis <wildfiction@gmail.com>",
  "bugs": {
    "url": "https://github.com/guyellis/ipapi/issues"
  },
  "dependencies": {
    "@maxmind/geoip2-node": "1.6.0",
    "csv-parse": "4.12.0",
    "decompress": "4.2.1",
    "express": "4.17.1",
    "fast-glob": "3.2.4",
    "node-fetch": "2.6.1",
    "p-map": "4.0.0",
    "validator": "13.1.17"
  },
  "description": "Web Server that provides a simple API for IP address information",
  "devDependencies": {
    "@types/express": "4.17.8",
    "@types/node-fetch": "2.5.7",
    "@types/validator": "13.1.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "eslint": "7.12.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "husky": "4.3.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && npm run lint"
    }
  },
  "homepage": "https://github.com/guyellis/ipapi",
  "keywords": [
    "ip",
    "address",
    "lookup",
    "api"
  ],
  "license": "MIT",
  "main": "dist/lib/api",
  "name": "ipapi",
  "public": true,
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/ipapi.git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts . --max-warnings 0",
    "lintfix": "npm run lint -- --fix",
    "prepublishOnly": "tsc",
    "prodrestart": "npm i && tsc && pm2 delete ipapi && pm2 start ../site-ops/pm2/ipapi.config.js --env production",
    "prodstart": "npm i && tsc && pm2 start ../site-ops/pm2/ipapi.config.js --env production",
    "prodstop": "pm2 stop ipapi",
    "start": "tsc && node dist/server.js",
    "test": "tsc && npm run lint"
  },
  "version": "4.0.0"
}
